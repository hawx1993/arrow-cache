{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "rootDir": ".",
    "outDir": "dist",
    "jsx": "react",
    "types": ["react", "react-dom"],
    "lib": ["DOM"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
